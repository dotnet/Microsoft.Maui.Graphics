{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es6",
    "module": "commonjs",
    "lib": ["es2016", "dom"],
    "strict": true,
    "alwaysStrict": true,
    "preserveConstEnums": true
  },
  "exclude": [
    "node_modules"
  ]
}
