<Window x:Class="GraphicsTester.WPF.Skia.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:graphics1="clr-namespace:Elevenworks.Graphics;assembly=Elevenworks.Graphics.Skia.Windows"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250" />
            <ColumnDefinition Width="5" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <ListView x:Name="List" />
        <GridSplitter Grid.Column="1" Width="5" HorizontalAlignment="Stretch" />
        <graphics1:WDSkiaGraphicsView x:Name="GraphicsView" Grid.Column="2" />
    </Grid>
</Window>
