<Project>
  <!--Package versions-->
  <PropertyGroup>
    <MicrosoftDotnetSdkInternalPackageVersion>6.0.100-rc.1.21452.64</MicrosoftDotnetSdkInternalPackageVersion>
    <MicrosoftAndroidSdkWindowsPackageVersion>31.0.100-rc.1.17</MicrosoftAndroidSdkWindowsPackageVersion>
    <MicrosoftMacCatalystSdkPackageVersion>15.0.100-rc.1.1532</MicrosoftMacCatalystSdkPackageVersion>
    <MicrosoftiOSSdkPackageVersion>15.0.100-rc.1.1532</MicrosoftiOSSdkPackageVersion>
    <MicrosofttvOSSdkPackageVersion>15.0.100-rc.1.1532</MicrosofttvOSSdkPackageVersion>
    <MicrosoftmacOSSdkPackageVersion>12.0.100-rc.1.1532</MicrosoftmacOSSdkPackageVersion>
  </PropertyGroup>
  <PropertyGroup>
    <!-- Trim all characters after first `-` or `+` is encountered. -->
    <DotNetPreviewVersionBand Condition=" '$(DotNetPreviewVersionBand)' == '' ">$([System.Text.RegularExpressions.Regex]::Replace($(MicrosoftDotnetSdkInternalPackageVersion), `[-+].*$`, ""))</DotNetPreviewVersionBand>
  </PropertyGroup>
</Project>
