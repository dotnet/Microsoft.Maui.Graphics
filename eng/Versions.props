<Project>
  <!--Package versions-->
  <PropertyGroup>
    <MicrosoftDotnetSdkInternalPackageVersion>6.0.100-preview.5.21275.1</MicrosoftDotnetSdkInternalPackageVersion>
    <MicrosoftAndroidSdkWindowsPackageVersion>30.0.100-preview.5.19</MicrosoftAndroidSdkWindowsPackageVersion>
    <MicrosoftMacCatalystSdkPackageVersion>14.5.100-preview.5.890</MicrosoftMacCatalystSdkPackageVersion>
    <MicrosoftiOSSdkPackageVersion>14.5.100-preview.5.890</MicrosoftiOSSdkPackageVersion>
    <MicrosofttvOSSdkPackageVersion>14.5.100-preview.5.890</MicrosofttvOSSdkPackageVersion>
    <MicrosoftmacOSSdkPackageVersion>11.3.100-preview.5.890</MicrosoftmacOSSdkPackageVersion>
  </PropertyGroup>
  <PropertyGroup>
    <!-- Trim all characters after first `-` or `+` is encountered. -->
    <DotNetPreviewVersionBand Condition=" '$(DotNetPreviewVersionBand)' == '' ">$([System.Text.RegularExpressions.Regex]::Replace($(MicrosoftDotnetSdkInternalPackageVersion), `[-+].*$`, ""))</DotNetPreviewVersionBand>
  </PropertyGroup>
</Project>
